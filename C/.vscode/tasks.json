{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "C/C++: Build Multiplication Table Test",
            "command": "/usr/bin/gcc",
            "args": [
                "-Wall",
                "-Wextra",
                "-g3",
                "-DTESTING",
                "${workspaceFolder}/C/Pratice_problems/Multiplication_Table/test_multiplication_table.c",
                "${workspaceFolder}/C/Pratice_problems/Multiplication_Table/Multiplication_Table.c",
                "-o",
                "${workspaceFolder}/C/Pratice_problems/Multiplication_Table/output/test_multiplication_table"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task to build the multiplication table test program."
        },
        
    ]
}